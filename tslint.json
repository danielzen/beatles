{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "trailing-comma": false,
    "member-access": false,
    "curly": [true, "ignore-same-line"],
    "variable-name": [true, "allow-snake-case"]
  },
  "rulesDirectory": []
}
